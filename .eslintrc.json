{
  "extends": [
    "next/core-web-vitals",
    "./node_modules/standard/eslintrc.json",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "space-before-function-paren": "off",
    "space-in-brackets": "off",
    "comma-dangle": "off"
  },
  "plugins": ["prettier"],
  "globals": {
    "React": "writable"
  }
}
